    " n = int(input())\n",
    "student_marks = {}\n",
    "for _ in range(n):\n",
    "    name, *line = input().split()\n",
    "    scores = list(map(float, line))\n",
    "    student_marks[name] = scores\n",
    "query_name = input()\n",
    "e=student_marks[query_name]\n",
    "f=sum(e)/len(e)\n",
    "print('%.2f' % f)"
   ]
  }
